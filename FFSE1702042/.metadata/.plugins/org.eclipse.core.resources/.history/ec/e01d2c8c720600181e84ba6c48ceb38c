package tung;
import java.util.Scanner;
public class Test {

	public static void main(String[] args) {
		int num=1;
			 Scanner myInput=new Scanner(System.in);
			 do {
		       System.out.print("Bạn chưa thuộc Bảng cửu chương nào nhập vào 1 số:");
		        int i=myInput.nextInt();
		       //int num=1;
		        while(num<=10) {
		           for(int j=1;j<=i;j++) {
		           System.out.print(j + " x " + num + " = " + (num*j));
		           System.out.print("		");
		           
		        	}
		           num++;
		           System.out.println();
		    }
			 }while(num<=11);
	}
}
