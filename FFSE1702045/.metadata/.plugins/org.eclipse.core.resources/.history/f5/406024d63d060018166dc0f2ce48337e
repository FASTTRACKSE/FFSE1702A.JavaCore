import java.util.Scanner;

public class nhapsoco2chuaso {

	public static void main(String[] args) {
		 Scanner bien = new Scanner(System.in);
		 System.out.println("Nhap vao 1 so co 2 chu so : ");
	       int n = bien.nextInt();
	       System.out.println(n);
	    
	       System.out.println("so hang chuc");
	      int i=Math.floor(n/10));
	       System.out.println("so hang don vi");
	       System.out.println(n-Math.floor(n/10)*10);
	       int a[]={MOT,HAI,BA,BON,NAM,SAU,BAY,TAM,CHIN};
	       int b[]={MOT,HAI,BA,BON,NAM,SAU,BAY,TAM,CHIN};
	       System.out.println(a[i]+"muoi"+b[i]);  
	}

}
