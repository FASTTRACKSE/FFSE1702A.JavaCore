import java.util.Scanner;
public class cuuchuong {
	
	public static void main(String[] args) {
		 Scanner bien = new Scanner(System.in);
		 System.out.println("Nhap vao 1 so : ");
		 
	       int n = bien.nextInt();
	       for ( int i = 1; i<= n ; i ++) {
	            for ( int j = 1; j<= 10 ; j ++) {
	                System.out.print(i  + " x " + j + " = " + i * j + "\t");
	            }
	            
	       }
	}
}