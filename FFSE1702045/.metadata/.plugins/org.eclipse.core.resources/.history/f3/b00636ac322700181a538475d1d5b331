package QuanLySinhVien;

import java.io.FileOutputStream;
import java.io.IOException;
import java.io.ObjectOutputStream;
import java.util.ArrayList;

import QLcanbo.canboinfo;

public class ObjectPtrint {
	
    public static void main(String[] args) throws IOException, ClassNotFoundException {
    	
    	  ArrayList<canboinfo> arrcanbo = new ArrayList<>();
    	  FileOutputStream fos = new FileOutputStream("fileobject.dat");
    	 ObjectOutputStream oos = new ObjectOutputStream(fos);
    	
    	  for (canboinfo cb : arrcanbo) {
              System.out.println(cb.toString());
          }
           
          long tongLuong = 0;
		for (canboinfo cb : arrcanbo) {
              long luong = cb.tinhLuong();
              tongLuong += luong;
          }
          System.out.println("Tổng lương phải trả cho cán bộ trong trường = " + tongLuong);
      }
   oos.writeObject(arrcanbo);
   oos.close();
    	 
    	
    }

