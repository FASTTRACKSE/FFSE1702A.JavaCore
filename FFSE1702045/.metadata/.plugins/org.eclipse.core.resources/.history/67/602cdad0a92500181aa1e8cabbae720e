package QLcanbo;

public class canboinfoexception extends Exception {
	String err;

	public canboinfoexception(String err) {
		super();
		this.err = err;
	}
	@Override
	public String toString() {
		return "Loi"+this.err;
	}
	static void chkHoten(String hoTen) {
		if(hoTen.equals("")) {
			throw new  canboinfoexception("Ten rong");
		}
	}

}