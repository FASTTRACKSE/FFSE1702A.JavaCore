import java.util.Scanner;
public class nhapsoco2chuaso {
	public static void main(String args[]) {
		String[] aryString = new String[10] ;
		aryString[0] ="không";
		aryString[1] ="một";
		aryString[2] ="hai";
		aryString[3] ="ba";
		aryString[4] ="bốn";
		aryString[5] ="năm";
		aryString[6] ="sáu";
		aryString[7] ="bảy";
		aryString[8] ="tám";
		aryString[9] ="chín"; 
		Scanner bien = new Scanner(System.in);
		System.out.println("Nhập 1 số có 2 chữ số");
		int n = bien.nextInt();
		int x = n/1000;
		int y = (n-x*1000)/100;
		int z = (n-x*1000-y*100)/10;
		int k = (n-x*1000-y*100-z*10);
	
		if(n<1000)
		{
			
			if(z==0 && k==0) {
				System.out.println(aryString[y] +" "+"trăm");
			}
			else if(z==1 && k>0) {
				System.out.println(aryString[y] +" "+"trăm" + " " +aryString[z]+" "+"mười" +" "+aryString[k] );
			}
			else if(z==1 && k==0) {
				System.out.println(aryString[y] +" "+"trăm" + " " +aryString[z]+" "+"mười" 
			}
			else if(z==0) {
				System.out.println(aryString[y] +" "+"trăm linh" + " " +aryString[k] );
			}
			else if(z>0 && k==1) {
				System.out.println(aryString[y] +" "+"trăm" + " " +aryString[z]+" "+"mươi mốt" );
			}
			else 
			{
			System.out.println(aryString[y] + " " + "trăm" + " " + aryString[z] + " " + "mươi" + " " + aryString[k] );
			}
		}
		else {
		if(y==0 && z==0 && k==0) {
			System.out.println(aryString[x] +" "+"nghìn");
		}
		else if(z==0)
		{
			System.out.println(aryString[x] +" "+"nghìn" +" "+aryString[y] + " " + "trăm"
					+ " " +"linh" + " " + aryString[k] );	
		}
		else if(z>0 && k==1)
		{
			System.out.println(aryString[x] +" "+"nghìn" +" "+aryString[y] + " " + "trăm"
				+" "	+ aryString[z]	+ " " +"mươi mốt" );	
		}
		else if(k==0)
		{
			System.out.println(aryString[x] +" "+"nghìn" +" "+aryString[y] + " " + "trăm"
					+ " " + aryString[z] + " " + "mươi" );
		}

		
		else {
			System.out.println(aryString[x] +" "+"nghìn" +" "+aryString[y] + " " + "trăm"
					+ " " + aryString[z] + " " + "mươi" + " " + aryString[k] );
		}
	}
}
}